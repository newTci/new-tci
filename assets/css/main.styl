.no-gutter
	padding 0

h1
	font-family "Ubuntu"
	
.left-side-banner
	background-color #ddd
	text-align right
	height 700px
	z-index 1
	img
		position fixed 
		top 220px
		margin-left -150px
	
.right-side-banner
	text-align left
	background-color #ddd
	min-height 700px
	img
		position fixed 
		top 220px
		
	
.wrapper-center
